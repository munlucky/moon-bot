{
  "name": "moonbot",
  "version": "0.1.0",
  "description": "Local-first AI Agent System based on Moltbot framework",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "moonbot": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "dev:start": "node scripts/dev.mjs start --build",
    "dev:stop": "node scripts/dev.mjs stop",
    "dev:restart": "node scripts/dev.mjs restart --build",
    "dev:start:win": "powershell -ExecutionPolicy Bypass -File scripts/dev.ps1 start -Build",
    "dev:stop:win": "powershell -ExecutionPolicy Bypass -File scripts/dev.ps1 stop",
    "dev:restart:win": "powershell -ExecutionPolicy Bypass -File scripts/dev.ps1 restart -Build",
    "gateway": "node dist/gateway/index.js",
    "gateway:watch": "bun run src/gateway/index.ts",
    "discord": "node dist/discord-bot.js",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "cd tests/e2e/approval && vitest run",
    "test:e2e:watch": "vitest --config tests/e2e/vitest.config.ts",
    "test:e2e:ui": "vitest --config tests/e2e/vitest.config.ts --ui",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "cli": "node dist/cli.js"
  },
  "dependencies": {
    "@sinclair/typebox": "^0.33.0",
    "@slack/bolt": "^3.21.1",
    "@slack/types": "^2.19.0",
    "chalk": "^5.6.2",
    "chokidar": "^4.0.0",
    "cli-table3": "^0.6.5",
    "commander": "^14.0.2",
    "discord.js": "^14.15.0",
    "dotenv": "^17.2.3",
    "openai": "^4.80.0",
    "playwright": "^1.48.0",
    "ws": "^8.18.0",
    "zod": "^3.23.0"
  },
  "optionalDependencies": {
    "node-pty": "^1.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@playwright/test": "^1.58.1",
    "@types/node": "^22.0.0",
    "@types/ws": "^8.5.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@vitest/coverage-v8": "^4.0.18",
    "@vitest/ui": "4.0.18",
    "eslint": "^9.0.0",
    "msw": "^2.12.7",
    "prettier": "^3.3.0",
    "tsx": "^4.21.0",
    "typescript": "^5.5.0",
    "vitest": "^4.0.18"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "keywords": [
    "ai-agent",
    "local-first",
    "websocket",
    "json-rpc",
    "discord",
    "automation"
  ],
  "author": "",
  "license": "MIT"
}
